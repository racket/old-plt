      if test "${GL_PTHREAD}" = "yes" ; then
        X_EXTRA_LIBS="$X_EXTRA_LIBS -lGL -pthread"
        MZOPTIONS="$MZOPTIONS -DASSUME_FIXED_STACK_SIZE -DFIXED_STACK_SIZE=1048576"
        GC2OPTIONS="$GC2OPTIONS -DNO_GC_SIGNALS"
        MROPTIONS="$MROPTIONS -DUSE_GL"
      fi
    fi
  fi
fi

