      if test "${enable_quartz}" = "yes" ; then
        gcc_vers_three_one=`${CC} -v 2>&1 | grep "version 3.1"`
        if test "$gcc_vers_three_one" = "" ; then
          # gcc 3.3 and up
          GCC_VERSION_THREE_THREE="yes"
