all: ../gc2/ctok xsrc
	$(MAKE) mzscheme.prc

xsrcs: $(XSRCS)

xsrc:
	mkdir xsrc

segmap.h: $(XSRCS) $(srcdir)/partition.ss
	../mzscheme -qr $(srcdir)/partition.ss $(XMAPS)

xobjects: $(OBJS)

mzscheme.prc: xobjects mz-sections.ld
	$(PALMCC) $(OBJS) mz-sections.ld -lm

mz-sections.s: mz.def
	$(MULTIGEN) mz.def

