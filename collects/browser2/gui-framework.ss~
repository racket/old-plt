(require-library "refer.ss")
(require-library "macro.ss")
(require-library "function.ss")

(define WIDTH 800)
(define HEIGHT 800)
(define frame (make-object frame% "" #f WIDTH HEIGHT))
(define horiz-panel1 (make-object horizontal-pane% frame))
(define horiz-panel2 (make-object horizontal-pane% frame))
(define back-btn (make-object button% "Back" horiz-panel1 (lambda (x y) (void)) '(border)))
(define forward-btn (make-object button% "Forward" horiz-panel1 (lambda (x y) (void)) '(border)))
(define reload-btn (make-object button% "Reload" horiz-panel1 (lambda (x y) (void)) '(border)))
(define home-btn (make-object button% "Home" horiz-panel1 (lambda (x y) (void)) '(border)))
(define print-btn (make-object button% "Print" horiz-panel1 (lambda (x y) (void)) '(border)))
(define history-choice-listbox (make-object choice% "History:" (list "http://www.rice.edu/~bonfield") horiz-panel1 (lambda (x y) (void))))

(define open-url-textbox (make-object text-field% "Location:" horiz-panel2 (lambda (x y) (void)) "" '(single)))


(define text (make-object text%))
(define editor-canvas (make-object editor-canvas% frame text))

(send editor-canvas min-height HEIGHT)
(send frame show #t)