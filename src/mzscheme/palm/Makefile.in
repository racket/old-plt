CFLAGS = -I.

XFORM = ../mzscheme -rq $(srcdir)/../gc2/xform.ss --notes --palm  ../gc2/ctok "$(PALMCPP) $(CPPFLAGS) -DUSE_SENORA_GC $(PALMINCS)"
